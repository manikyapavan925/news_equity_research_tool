{
    "version": 2,
    "builds": [
        {
            "src": "src/app.py",
            "use": "@vercel/python@3.1.0",
            "config": {
                "maxLambdaSize": "500mb",
                "runtime": "python3.9",
                "installCommand": "python3.9 -m pip install --upgrade pip && pip install -r requirements.txt"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/app.py"
        }
    ],
    "env": {
        "PYTHONUNBUFFERED": "1",
        "PYTHONPATH": "/vercel/path0",
        "PYTHONUTF8": "1",
        "PYTHONIOENCODING": "utf-8"
    }
}
