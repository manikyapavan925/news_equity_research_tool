{
    "version": 2,
    "public": true,
    "builds": [
        {
            "src": "src/app.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/app.py"
        }
    ],
    "functions": {
        "src/app.py": {
            "memory": 3008,
            "maxDuration": 60
        }
    }
}
