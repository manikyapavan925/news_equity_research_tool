{
    "version": 2,
    "builds": [
        {
            "src": "src/app.py",
            "use": "@vercel/python@3.1.0",
            "config": {
                "maxLambdaSize": "1mb",
                "runtime": "python3.11",
                "installCommand": "pip install -r requirements.txt --no-cache-dir"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/app.py"
        }
    ],
    "env": {
        "PYTHONUNBUFFERED": "1",
        "PYTHONPATH": "/vercel/path0",
        "PIP_NO_CACHE_DIR": "1"
    }
}
