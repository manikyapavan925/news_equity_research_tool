{
    "version": 2,
    "public": true,
    "builds": [
        {
            "src": "src/app.py",
            "use": "@vercel/python",
            "config": {
                "maxDuration": 60,
                "memory": 3008,
                "pythonVersion": "3.9"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/app.py"
        }
    ]
}
