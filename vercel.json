{
    "version": 2,
    "builds": [
        {
            "src": "src/app.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.11",
                "installCommand": "python3 -m pip install --upgrade pip && pip install -r requirements.txt"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/app.py"
        }
    ],
    "env": {
        "PYTHONUNBUFFERED": "1",
        "PYTHONPATH": "/vercel/path0"
    }
}
