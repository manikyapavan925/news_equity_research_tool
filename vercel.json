{
    "version": 2,
    "builds": [
        {
            "src": "src/app.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "500mb",
                "runtime": "python3.9",
                "installCommand": "python -m pip install --upgrade pip && pip install -r requirements.txt --no-cache-dir"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/app.py"
        }
    ],
    "env": {
        "PYTHONUNBUFFERED": "1",
        "PYTHONPATH": "/vercel/path0",
        "PIP_NO_CACHE_DIR": "off"
    }
}
